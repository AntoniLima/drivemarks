<!doctype html>
<html>
<head>
  <title>drivemarks</title>
  <meta charset="utf-8" />
  <link rel="stylesheet" type="text/css" href="/public/css/base.css">
</head>
<body>
  <header>drivemarks</header>
  <div class="bookmark">
    <div class="title"><h1>${title} is bookmarked!</h1></div>
    <div class="link"><a href="${description}">${description}</a></div>
    <a target="_blank" class="viewButton" href="https://drive.google.com/#folders/${parents[0].id}">View your bookmarks on Google Drive</a>
  </div>
</body>
</html>
